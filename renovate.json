{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "baseBranches": ["develop"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "stabilityDays": 14
      "automerge": true
    },
    {
      "matchPackageNames": ["nuget"],
      "stabilityDays": 14
    }
  ]
}
